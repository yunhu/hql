				}
				//偶数行背景为灰色
				temp.push("<li "+(i % 2 == 0?"":"class='bg2'")+" onclick=\"flyItemClick('MU5160','2016-03-24')\"><div class='li_com'><span class='w180'> <img src='http://www.umetrip.com/img/aircorp/default.png' width='32' height='32' align='东方航空' /> <b>"
						+"<a href='javascript:void(0);' title='MU5160'>MU5160</a></b><br />"+
						"<a href='javascript:void(0);' title='东方航空'>东方航空</a>"+
					"</span> <span class='w125'> 21:40</span><span class='w125'> --</span> <span class='w125'>"+
						"T2/T2</span> <span class='w125'>"+
						"23:55</span> <span class='w125'>--</span> <span class='w125'>"+
						"计划</span> <span class='fob'><a href=\"fc.do?&channel="+ channel +"&flightNo=MU5160&date=2016-03-24\">详情</a></span><div class='clear'></div></div></li>");
				i++;
			
			
				//最后一页，不是完整页
				if(temp.length > 0){
					page.push(temp.join(""));
				}
				$("#list").html(page[0]);
				
				lib.genPage(page, size);
				
        	if(channel == 'kzjtw'){				
				$('#header').hide();
				$('#footer').hide();
				$('#sidebar').hide();
				$('.main').css({'padding-top':'5px','padding-bottom':'5px'});
			}
        	}
        });
    </script>
</body>
</html>




